@model PackItUI.Areas.App.Models.AboutViewModel
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewData["Title"] = "About";
}
<h2>@ViewData["Title"]</h2>

<table class="table">
    <caption>Service Versions</caption>
    <tr>
        <th scope="col">Service</th>
        <th scope="col">Version</th>
        <th scope="col">About</th>
    </tr>
    @foreach (KeyValuePair<string, PackItUI.Services.ServiceInfo> x in Model.Services)
    {
    <tr>
        <td>@x.Key</td>
        <td>@x.Value.Version</td>
        <td>@x.Value.About</td>
    </tr>
    }
</table>
